{
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/en",
      "destination": "/"
    },
    {
      "source": "/docs",
      "destination": "/en/introduction"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/xHdCpcPHRE"
    },
    {
      "source": "/github",
      "destination": "https://github.com/t3-oss/create-t3-app"
    },
    {
      "source": "/showcase",
      "destination": "/en/t3-collection"
    },
    {
      "source": "/collection",
      "destination": "/en/t3-collection"
    },
    {
      "source": "/awesome",
      "destination": "/en/t3-collection"
    }
  ],
  "rewrites": [
    {
      "source": "/upgrade/:path*",
      "destination": "https://t3-upgrade.vercel.app/:path*"
    }
  ]
}
